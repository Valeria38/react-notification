{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "es5",
    "allowJs": false,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "utils/*": ["utils/*"],
      "utils": ["utils"],
      "components/*": ["components/*"],
      "components": ["components"],
      "images/*": ["images/*"],
      "images": ["images"],
      "constants/*": ["constants/*"],
      "constants": ["constants"]
    },
    "jsx": "react"
  }
}
